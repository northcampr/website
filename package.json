    "@vuepress/plugin-pwa": "^1.8.2",
    "@vuepress/plugin-pwa-popup": "^2.0.0-alpha.8",
    "@vuepress/utils": "^2.0.0-alpha.1",
    "luxon": "^2.5.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "vuepress": "^2.0.0-beta.6",
    "webpack": "5.28.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
